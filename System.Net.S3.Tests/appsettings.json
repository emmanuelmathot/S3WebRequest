{
    "AllowedHosts": "*",
    "AWS": {
        "Profile": "localstack",
        "Region": "eu-central-1",
        "ServiceURL": "http://localhost:4566"
    },
    "LocalStack": {
        "Enabled": true
    },
    "Logging": {
        "LogLevel": {
            "Default": "Debug",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "Serilog": {
        "MinimumLevel": "Debug",
        "Using": [
            "Serilog.Sinks.File"
        ],
        "WriteTo": [
            {
                "Name": "File",
                "Args": {
                    "Path": "testcontainers.log"
                }
            }
        ]
    }
}