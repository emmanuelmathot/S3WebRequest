{
    "AllowedHosts": "*",
    "AWS": {
        "Profile": "minio",
        // "Region": "eu-central-1",
        "ServiceURL": "https://api.charter.uat.esaportal.eu"
        // "Profile": "ovh",
        // "Region": "fr-par",
        // "AuthenticationRegion": "fr-par",
        // "ServiceURL": "https://s3.fr-par.scw.cloud"
    },
    "LocalStack": {
        "Enabled": false
    },
    "Logging": {
        "LogLevel": {
            "Default": "Debug",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "Serilog": {
        "MinimumLevel": "Debug",
        "Using": [
            "Serilog.Sinks.File"
        ],
        "WriteTo": [
            {
                "Name": "File",
                "Args": {
                    "Path": "testcontainers.log"
                }
            }
        ]
    }
}