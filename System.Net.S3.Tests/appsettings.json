{
    "AllowedHosts": "*",
    "AWS":{
        "Profile": "LocalStack",
        "Region": "eu-central-1",
        "AccessKey": "localkey",
        "SecretKey": "localsecret",
        "ServiceURL": "http://localhost:4566"
    },
    "Serilog": {
        "MinimumLevel": "Debug",
        "Using": [
            "Serilog.Sinks.File"
        ],
        "WriteTo": [
            {
                "Name": "File",
                "Args": {
                    "Path": "testcontainers.log"
                }
            }
        ]
    }
}